{
    "name": "imtiaz/sslcommerz",
    "description": "you can easily intregate sslcommerz in laravel",
    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Imtiaz\\Sslcommerz\\": "src/"
        }
    },
    "authors": [
        {
            "name": "imtiaz Ikbal",
            "email": "imtiaz.ikbal.com@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
                "php": "^7.4",
        "illuminate/support": "*",
        "laravel/framework":"^10"
    },
    "extra": {
    "laravel": {
        "providers": [
            "Imtiaz\\Sslcommerz\\SslcommerzServiceProvider"
        ],
        "aliases": {
            "Sslcommerz":"Imtiaz\\Sslcommerz\\Sslcommerz"
        }
    }
}
}
